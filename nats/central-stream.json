{
    "name": "dataleaf",
    "mirror": {
        "name": "data",
        "external": {
            "api": "$JS.central.API"
        }
    },
    "retention": "limits",
    "max_consumers": -1,
    "max_msgs_per_subjects": -1,
    "max_msgs": -1,
    "max_bytes": -1,
    "storage": "file",
    "discard": "old",
    "num_replicas": 2,
    "duplicate_window": 120000000000,
    "sealed": false,
    "deny_delete": false,
    "deny_purge": false,
    "allow_rollup_hdrs": false,
    "allow_direct": false,
    "mirror_direct": false
}
